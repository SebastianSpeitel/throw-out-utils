{
  "extends": [
    "prettier",
    "plugin:@typescript-eslint/recommended","plugin:@typescript-eslint/recommended",
    "prettier/@typescript-eslint"
  ],
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "ecmaVersion": 2018,
    "sourceType": "module"
  },
  "rules": {
    "@typescript-eslint/explicit-module-boundary-types": "off"
  },
  "env": {
    "es6": true
  }
}